<Window x:Class="ULogView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ULogView"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        mc:Ignorable="d"
        d:DesignHeight="400" d:DesignWidth="300"		
        Title="MainWindow" 
		Loaded="Window_Loaded">
    <DockPanel>
		<Button DockPanel.Dock="Top" Click="Button_Click">Knopf</Button>
		<wv2:WebView2 Name="webView" Source="http://localhost:3000" NavigationCompleted="webView_NavigationCompleted" />
	</DockPanel>
</Window>
